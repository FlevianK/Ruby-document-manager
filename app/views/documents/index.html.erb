<h1>Listing documents</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @documents.each do |document| %>
    <tr>
      <td><%= document.title %></td>
      <td><%= document.text %></td>
      <td><%= link_to 'Show', document_path(document) %></td>
    </tr>
  <% end %>
</table>